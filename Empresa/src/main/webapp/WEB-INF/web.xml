
<web-app
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
    version="4.0">
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
   	<description></description>
  	<display-name>ActualizaNominas</display-name>
  	<servlet-name>ActualizaNominas</servlet-name>
  	<servlet-class>servlets.ActualizaNominas</servlet-class>
  </servlet>
  <servlet>
  <description></description>
  	<display-name>AltaEmpleados</display-name>
  	<servlet-name>AltaEmpleados</servlet-name>
  	<servlet-class>servlets.AltaEmpleados</servlet-class>
  </servlet>
  <servlet>
  	<description></description>
  	<display-name>EmpleadosLista</display-name>
  	<servlet-name>EmpleadosLista</servlet-name>
  	<servlet-class>servlets.EmpleadosLista</servlet-class>
  </servlet>
  <servlet>
  	<description></description>
  	<display-name>MostrarSalario</display-name>
  	<servlet-name>MostrarSalario</servlet-name>
  	<servlet-class>servlets.MostrarSalario</servlet-class>
  </servlet>
  <servlet>
 	<description></description>
  	<display-name>MostrarEmpleado</display-name>
  	<servlet-name>MostrarEmpleado</servlet-name>
  	<servlet-class>servlets.MostrarEmpleado</servlet-class>
  </servlet>
  <servlet>
  	<description></description>
  	<display-name>ModificarEmpleado</display-name>
  	<servlet-name>ModificarEmpleado</servlet-name>
  	<servlet-class>servlets.ModificarEmpleado</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>ActualizaNominas</servlet-name>
  	<url-pattern>/ActualizaNominas</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AltaEmpleados</servlet-name>
  	<url-pattern>/AltaEmpleados</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>EmpleadosLista</servlet-name>
  	<url-pattern>/EmpleadosLista</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>MostrarSalario</servlet-name>
  	<url-pattern>/MostrarSalario</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>MostrarEmpleado</servlet-name>
  	<url-pattern>/MostrarEmpleado</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ModificarEmpleado</servlet-name>
  	<url-pattern>/ModificarEmpleado</url-pattern>
  </servlet-mapping>
  <error-page>
  	<exception-type>java.lang.Throwable</exception-type>
  	<location>/errorPage.jsp</location>
  </error-page>
</web-app>
